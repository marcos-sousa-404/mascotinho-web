{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    ["@babel/preset-react",
    {
      "runtime": "automatic"
    }
    ]
  ],
  "plugins": [
    "react-hot-loader/babel",
    [
      "babel-plugin-styled-components",
      {
        "displayName": true,
        "fileName": true,
        "namespace": "my-app"
      }
    ]
  ]
}